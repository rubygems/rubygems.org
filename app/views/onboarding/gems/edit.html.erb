<%= form_with(model: @organization_onboarding, url: onboarding_gems_path, method: :patch) do |form| %>
  <h1>Edit Gems</h1>
  <%= form.select :rubygems, options_from_collection_for_select(@organization_onboarding.avaliable_rubygems, :id, :name), multiple: true, include_hidden: false %>
  <br/>
  <%= form.submit %>
<% end %>
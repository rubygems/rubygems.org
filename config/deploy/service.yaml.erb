apiVersion: v1
kind: Service
metadata:
  name: web
  labels:
    name: web
  annotations:
    service.beta.kubernetes.io/aws-load-balancer-internal: 0.0.0.0/0
    service.beta.kubernetes.io/aws-load-balancer-name: <%= environment %>-rubygems-org
spec:
  type: LoadBalancer
  ports:
  - port: 80
    targetPort: 8080
  selector:
    name: web
